<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="205dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TableLayout
        android:id="@+id/horarios"
        android:layout_width="300dp"
        android:layout_height="200dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/borde_tabla_exterior_shape">

        <TableRow>
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_superior"
                android:text="Días" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_superior"
                android:text="Horario" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_superior"
                android:text="Edad" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_superior"
                android:text="Precio" />
        </TableRow>

        <TableRow>
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="L/X" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="19h-20h" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="14-16" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="20€/día" />
        </TableRow>

        <TableRow>
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="M/J" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="17h-18h" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="17-19" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="20€/día" />
        </TableRow>

        <TableRow>
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="L/X" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="20h-22h" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="19-22" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="25€/día" />
        </TableRow>

        <TableRow>
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="" />
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:background="@drawable/borde_tabla_interior"
                android:text="" />
        </TableRow>

    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>